<Page x:Class="Generation.ThirdPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Generation"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="517"
      Title="ThirdPage">

    <Grid Background="#FFBDA0FF">
        <Grid.RowDefinitions>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="100*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <TextBlock Grid.ColumnSpan="2" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="20" FontFamily="Bodoni MT Black" Width="449" Margin="58,0,0,0"><Run Text="Тестов не имеется, на данный момент"/><LineBreak/><Run/><LineBreak/></TextBlock>
        <Border BorderThickness="1" Grid.ColumnSpan="2"/>
        <Frame x:Name="FrameForNoTest" Grid.RowSpan="2" Grid.ColumnSpan="2"/>

    </Grid>
</Page>
